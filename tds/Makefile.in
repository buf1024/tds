CC = @CC@ 
CCFLAGS = @CCFLAGS@
LIB_CCFLAGS=@LIB_CCFLAGS@
LIB_LDFLAGS=@LIB_LDFLAGS@
ARCHIVE = @ARCHIVE@
AR_FLAGS = @AR_FLAGS@

###################################################################################
#write your own rules here                                                        #
###################################################################################
PROG=libtds.a
OBJS=myqueue.o

EXT_CCFLAGS =  
EXT_LDFLAGS = -ldl

all: $(PROG)

$(PROG):$(OBJS)
	$(ARCHIVE) $(AR_FLAGS) $@ $(OBJS)
	ranlib $@


install:$(PROG)
	cp $(PROG) @prefix@/lib

clean:
	rm -f *.o 
	rm -f $(PROG)

include @prefix@/rule.mk
