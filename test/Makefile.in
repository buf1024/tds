PROG=tds_test

OBJS=main.o myqueue_test.o dict_test.o

EXT_CCFLAGS= -I../tds
EXT_LDFLAGS= -L../tds -ltds

all:$(PROG)

$(PROG): $(OBJS)
	$(CC) $(CCFLAGS) $(EXT_CCFALGS) -o $@ $(OBJS) $(EXT_LDFLAGS)

install:$(PROG)
	rm -f @prefix@/bin/$(PROG)
	cp $(PROG) @prefix@/bin/$(PROG)

clean:
	rm -f *.o
	rm -f $(PROG)

include @prefix@/rule.mk

